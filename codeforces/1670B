#include<numeric>


#define ull unsigned long long
#define ll long long
#define NUM 1000000007
//ll mod  = 1000000007;
using namespace std;



ll binpow(ll x, ll y)
{
    ll res = 1ll; // Initialize result
  
    while (y > 0ll) {
        // If y is odd, multiply x with result
        if (y & 1ll)
            res = ((res%NUM) * (x%NUM))%NUM;
  
        // y must be even now
        y = y >> 1; // y = y/2
        x = ((x%NUM) * (x%NUM))%NUM; // Change x to x^2
    }
    return res%NUM;
}

bool sortbysec(pair<ll,ll> &a , pair<ll,ll> &b)
{
    if(a.first == b.first)
    {
        return (a.second <= b.second);
    }
    return (a.first < b.first) ;
}
 
int binarysearch(vector<int> v, int find,int l, int r)
{
    if(find<v[l] or find > v[r]) 
    {
        return -1;
    }
    while(l<=r)
    {
        int mid  = l + (r-l)/2 ;
        if(v[mid] == find) return mid;
        if(v[mid]  > find) r = mid -1;
        else l =mid +1;
    }
    return -1;
}

void solve()
{
    int n;
    string s;
    cin>>n;
    cin>>s;
    int m;
    cin>>m;
    if(m==26)
    {
        cout<<1<<endl;
        return;
    }
    int ans = INT_MIN;
    //map<char,int> mp;
    vector<bool> v1(26,false);
    for(int i=0;i<m;i++)
    {
        char c;
        cin>>c;
        //mp[c]++;
        v1[c-'a'] = true;
    }
    vector<int> count;
    int check =0;
    for(int i=0;i<n;i++)
    {
        if(!v1[s[i]-'a'])
        {
            check++;
        }
        else
        {
            count.push_back(check);
            if(count.size() == 1)
            {
                ans =max(ans,check);
            }
            else ans = max(ans,check+1);
            check = 0;
        }
    }
    if(count.empty())
    {
        cout<<0<<endl;
        return;
    }
    cout<<ans<<endl;

}
int main()
{
    
    
    int t ;
    cin>>t;
    while(t--)
    {
        solve();
    }
    return 0;
    
}
