#define ull unsigned long long
#define ll long long
//#define NUM 1000000001
//ull mod  = 998244353;
using namespace std;

ll binarysearch(ll num, vector<ll> v)
{
    ll l =0;
    ll r = v.size() -1;
    if(num <= v[0]) return  -1;
    //if(num > v[r]) return -1;
    ll ans = -1;
    while(l<=r)
    {
        ll mid = l +(r-l)/2;
        //if(v[mid] == num) return mid;
        if(v[mid]<num)
        {
            l = mid + 1;
            ans =mid;

        }
        else
        {
            r = mid -1;
            //ans =mid;
        }
    }
    /*for(ll i= ans+1 ;i<r;i++)
    {
        if(v[i] == num) ans++;
    }*/
    return ans;
}
int check(int t, vector<int> cnt )
{
    ll free =0,need =0;
    for(int i=1;i<cnt.size();i++)
    {
        if(t >= cnt[i])
        {
            free += (ll) (t-cnt[i])/2;
        }
        else
        {
            need += (ll) cnt[i] - t;
        }
    }
    if(free >= need) return 1;
    return 0;
}
int main()
{
    ll t;
    cin>>t;
    while(t--)
    {
        int n,m;
        cin>>n>>m;
        vector<int> v;
        for(int i=0;i<m;i++)
        {
            int temp;
            cin>>temp;
            v.push_back(temp);
        }
        vector<int> cnt(n+1,0);
        for(int i=0;i<m;i++)
        {
            cnt[v[i]]++;
        }
        int l=0;
        int ans =2*m;
        int r = 2*m;
        while(l<=r)
        {
            int m = l+ (r-l)/2;
            if(check(m,cnt))
            {
                ans = m;
                r =m -1;
            }
            else l = m + 1;
        }
        cout<<ans<<endl;

    }
    return 0;
}
