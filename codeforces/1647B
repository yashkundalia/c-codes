#define ull unsigned long long
#define ll long long
#define NUM 1000000007
//ll mod  = 1000000007;
using namespace std;



ll binpow(ll x, ll y)
{
    ll res = 1ll; // Initialize result
  
    while (y > 0ll) {
        // If y is odd, multiply x with result
        if (y & 1ll)
            res = ((res%NUM) * (x%NUM))%NUM;
  
        // y must be even now
        y = y >> 1; // y = y/2
        x = ((x%NUM) * (x%NUM))%NUM; // Change x to x^2
    }
    return res%NUM;
}

bool sortbysec(pair<ll,ll> &a , pair<ll,ll> &b)
{
    if(a.first == b.first)
    {
        return (a.second <= b.second);
    }
    return (a.first < b.first) ;
}
 
int binarysearch(vector<int> v, int find,int l, int r)
{
    if(find<v[l] or find > v[r]) 
    {
        return -1;
    }
    while(l<=r)
    {
        int mid  = l + (r-l)/2 ;
        if(v[mid] == find) return mid;
        if(v[mid]  > find) r = mid -1;
        else l =mid +1;
    }
    return -1;
}
bool isvalid(int i, int j, int n, int m)
{
    if(i<0 or j<0 or i>=n or j>=m)
    {
        return false;
    }
    return true;
}
void bin(ll n)
{
    /* step 1 */
    if (n > 1)
        bin(n / 2);
 
    /* step 2 */
    cout << n % 2;
}

void solve()
{
    
    int n,m;
    cin>>n>>m;
    vector<string> v(n);
    for(int i=0;i<n;i++)
    {
        
        string temp;
        cin>>temp;
        v[i] = temp;
        
    }
    for(int i=0;i<n;i++)
    {
        bool flag =true;
        for(int j=0;j<m;j++)
        {

            if(isvalid(i-1,j,n,m) and isvalid(i,j+1,n,m) and isvalid(i-1,j+1,n,m))
            {
                //cout<<"herre"<<i<<"j"<<j<<endl;
                //cout<<v[i][j]<<" "<<v[i][j+1]<<" "<<v[i-1][j]<<" "<<v[i-1][j+1]<<endl;
                if(v[i][j] == '1' and v[i-1][j] == '1' and v[i][j+1] == '1' and v[i-1][j+1] == '0')
                {
                    flag = false;
                    cout<<"NO"<<endl;
                    return;

                }
                if(v[i][j] == '1' and v[i-1][j] == '1' and v[i][j+1] == '0' and v[i-1][j+1] == '1')
                {
                    flag = false;
                    cout<<"NO"<<endl;
                    return;

                }
                if(v[i][j] == '1' and v[i-1][j] == '0' and v[i][j+1] == '1' and v[i-1][j+1] == '1')
                {
                    flag = false;
                    cout<<"NO"<<endl;
                    return;

                }
                if(v[i][j] == '0' and v[i-1][j] == '1' and v[i][j+1] == '1' and v[i-1][j+1] == '1')
                {
                    flag = false;
                    cout<<"NO"<<endl;
                    return;

                }
            }
        }

    }
    cout<<"YES"<<endl;
    return;
}


int main()
{
    

    int t ;
    cin>>t;
    //t=1;
    while(t--)
    {
        solve();
    }
    return 0;
    
}
