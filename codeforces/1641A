#define ull unsigned long long
#define ll long long
#define NUM 1000000007
//ll mod  = 1000000007;
using namespace std;



ll binpow(ll x, ll y)
{
    ll res = 1ll; // Initialize result
  
    while (y > 0ll) {
        // If y is odd, multiply x with result
        if (y & 1ll)
            res = ((res%NUM) * (x%NUM))%NUM;
  
        // y must be even now
        y = y >> 1; // y = y/2
        x = ((x%NUM) * (x%NUM))%NUM; // Change x to x^2
    }
    return res%NUM;
}

bool sortbysec(pair<ll,ll> &a , pair<ll,ll> &b)
{
    if(a.first == b.first)
    {
        return (a.second <= b.second);
    }
    return (a.first < b.first) ;
}
 
int binarysearch(vector<int> v, int find,int l, int r)
{
    if(find<v[l] or find > v[r]) 
    {
        return -1;
    }
    while(l<=r)
    {
        int mid  = l + (r-l)/2 ;
        if(v[mid] == find) return mid;
        if(v[mid]  > find) r = mid -1;
        else l =mid +1;
    }
    return -1;
}


void solve()
{
    
    ll n,x;
    cin>>n>>x;
    vector<ll> v;
    map<ll,ll> mp;
    for(ll i=0;i<n;i++)
    {
        ll temp;
        cin>>temp;
        v.push_back(temp);
        mp[temp]++;

    }
    sort(v.begin(),v.end());
    ll ans =0;
    for(ll i=0;i<n;i++)
    {
        if(mp[v[i]] > 0)
        {
            mp[v[i]]-- ;
            ll req = v[i]*x;
            if(mp[req]> 0)
            {
                mp[req]--;
            }
            else ans++; 
        }
        
    }
    /*for(auto i:mp)
    {
        cout<<
        ll num  = i.first;
        ll req = num*x;
        if(mp.find(req)!=mp.end())
        {
            if(mp[req] == 1) mp.erase(req);
            mp[req]--;
            mp[num]--;
        }
        else 
        {
            cout<<num<<endl;
            ans++;
        }
    }
    /*sort(v.begin(),v.end());
    vector<bool> check(n,false) ;
    for(int i=0;i<n;i++)
    {

    }*/
    cout<<ans<<endl;
    return;
}


int main()
{
    

    int t ;
    cin>>t;
    //t=1;
    while(t--)
    {
        solve();
    }
    return 0;
    
}
