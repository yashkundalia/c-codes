#define ull unsigned long long
#define ll long long
#define NUM 998244353
//ll mod  = 1000000007;
using namespace std;



ll binpow(ll x, ll y)
{
    ll res = 1ll; // Initialize result
  
    while (y > 0ll) {
        // If y is odd, multiply x with result
        if (y & 1ll)
            res = ((res%NUM) * (x%NUM))%NUM;
  
        // y must be even now
        y = y >> 1; // y = y/2
        x = ((x%NUM) * (x%NUM))%NUM; // Change x to x^2
    }
    return res%NUM;
}

bool sortbysec(pair<ll,ll> &a , pair<ll,ll> &b)
{
    if(a.first == b.first)
    {
        return (a.second <= b.second);
    }
    return (a.first < b.first) ;
}
 
int binarysearch(vector<int> v, int find,int l, int r)
{
    if(find<v[l] or find > v[r]) 
    {
        return -1;
    }
    while(l<=r)
    {
        int mid  = l + (r-l)/2 ;
        if(v[mid] == find) return mid;
        if(v[mid]  > find) r = mid -1;
        else l =mid +1;
    }
    return -1;
}
bool isvalid(ll i , ll j ,ll n,ll m)
{
    if(i<0 or i> n-1 or j<0 or j> m-1)
    {
        return false;
    }
    return true;
}
void dfs(vector<vector<int> > &adj, vector<bool> &check, int i)
{
    if(check[i]) return;
    check[i] = true;
    for(auto j : adj[i])
    {
        if(!check[j]) dfs(adj,check,j) ;
    }
    return;
}
void solve()
{
    ll n;
    cin>>n;
    vector<ll> v;
    //set
    vector<ll> count(n,0);
    for(ll i=0;i<n;i++)
    {
        ll temp;
        cin>>temp;
        v.push_back(temp);
    }
    sort(v.begin(),v.end());
    ll j= n-1;
    ll num = n-1;
    while(j>=0)
    {
        ll ind = lower_bound(v.begin(),v.end(),v[j]) - v.begin();
        if(ind==0) break;
        num = min(num,ind);
        //cout<<ind<<"ind"<<endl;
        while(j!= ind -1)
        {
            //if(num<=0) break;
            count[j] = num;
            //cout
            num--;
            j--;
        }
    }
    ll ans =0;
    for(ll i=0;i<n;i++)
    {
        //cout<<count[i]<<" ";
        if(count[i]> 0) ans++;
    }
    //cout<<endl;
    cout<<ans<<endl;
    return;

}



    

int main()
{
    
    
    int t ;
    //cin>>t;
    t=1;
    while(t--)
    {
    
        solve();
    }
    return 0;
    
}
