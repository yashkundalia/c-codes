#include <iostream>
#include <map>
#include<unordered_map>
#include <iterator>
#include <vector>
#include <string>
#include <algorithm>
#include<utility>
#include<climits>
#include<stack>

#define ull unsigned long long
#define ll long long
//#define NUM 1000000001
//ull mod  = 998244353;
using namespace std;


int main()
{
    ll t;
    cin>>t;
    while(t--)
    {
        ll n,x;
        cin>>n>>x;
        vector<ll> v;
        for(ll i=0;i<n;i++)
        {
            ll temp;
            cin>>temp;
            v.push_back(temp);
        }
        sort(v.begin(),v.end());
        ll ans =0;
        ll sum =0;
        for(ll i=0;i<n;i++)
        {
            sum += v[i];
            if(sum > x) break;
            ans += (x - sum)/(i+1) + 1;
        }
        cout<<ans<<endl;

    }
    return 0;
}
