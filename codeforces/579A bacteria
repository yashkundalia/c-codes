#include <iostream>
#include <map>
#include <iterator>
#include <vector>
#include <string>
#include <algorithm>
#include<utility>
#include<climits>



using namespace std;

int main()
{
    int n;
    cin>>n;
    int ans =1;
    while(n!=1)
    {
        ans += n%2;
        n = n/2;
    }
    cout<<ans<<endl;
    return 0;



}

