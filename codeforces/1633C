#define ull unsigned long long
#define ll long long
#define NUM 1000000007
//ll mod  = 1000000007;
using namespace std;



ll binpow(ll x, ll y)
{
    ll res = 1ll; // Initialize result
  
    while (y > 0ll) {
        // If y is odd, multiply x with result
        if (y & 1ll)
            res = ((res%NUM) * (x%NUM))%NUM;
  
        // y must be even now
        y = y >> 1; // y = y/2
        x = ((x%NUM) * (x%NUM))%NUM; // Change x to x^2
    }
    return res%NUM;
}

bool sortbysec(pair<ll,ll> &a , pair<ll,ll> &b)
{
    if(a.first == b.first)
    {
        return (a.second <= b.second);
    }
    return (a.first < b.first) ;
}
 
int binarysearch(vector<int> v, int find,int l, int r)
{
    if(find<v[l] or find > v[r]) 
    {
        return -1;
    }
    while(l<=r)
    {
        int mid  = l + (r-l)/2 ;
        if(v[mid] == find) return mid;
        if(v[mid]  > find) r = mid -1;
        else l =mid +1;
    }
    return -1;
}

void solve()
{
    ll hc,dc,hm,dm;
    cin>>hc>>dc;
    cin>>hm>>dm;
    ll k,w,a;
    cin>>k>>w>>a;
    ll h,a1;
    h = hc;
    a1 = dc;
    for(ll i=0;i<=k;i++)
    {
        h = hc + i*(a);
        a1 = dc + (k-i)*w;
        ll c1 = h/dm ;
        if(h%dm >0) c1++;
        ll c2 = hm/a1;
        if(hm%a1 >0) c2++;
        //cout<<c2<<endl;
        if(c1>=c2)
        {
            //cout<<c1<<" "<<c2<<endl;
            //cout<<i<<endl;
            cout<<"YES"<<endl;
            return;
        }

    }
    cout<<"NO"<<endl;
    return;

}


int main()
{
    

    int t ;
    cin>>t;
    //t=1;
    while(t--)
    {
        solve();
    }
    return 0;
    
}
