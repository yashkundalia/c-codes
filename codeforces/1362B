#define ull unsigned long long
#define ll long long
#define NUM 1000000007
//ll mod  = 1000000007;
using namespace std;



ll binpow(ll x, ll y)
{
    ll res = 1ll; // Initialize result
  
    while (y > 0ll) {
        // If y is odd, multiply x with result
        if (y & 1ll)
            res = ((res%NUM) * (x%NUM))%NUM;
  
        // y must be even now
        y = y >> 1; // y = y/2
        x = ((x%NUM) * (x%NUM))%NUM; // Change x to x^2
    }
    return res%NUM;
}

bool sortbysec(pair<ll,ll> &a , pair<ll,ll> &b)
{
    if(a.first == b.first)
    {
        return (a.second <= b.second);
    }
    return (a.first < b.first) ;
}
 
//vector<bool> ch(1025,false);
//vector<int> num(1025,0);
bool check(int k,vector<int> v,vector<bool> ch)
{
    int n = v.size();
    for(int i=0;i<n;i++)
    {
        if(!ch[v[i]^k]) return false;
    }
    return true;
}

void solve()
{
    int n;
    cin>>n;
    vector<int> v;
    vector<bool> ch(1025,false);
    for(int i=0;i<n;i++)
    {
        int temp;
        cin>>temp;
        ch[temp] = true;
        v.push_back(temp);
    }
    for(int i=1;i<1024;i++)
    {
        if(check(i,v,ch))
        {
            cout<<i<<endl;
            return;
        }
    }
    cout<<-1<<endl;
    return;


}




int main()
{
    
    /*freopen("input.txt", "r", stdin);
    freopen("output.txt", "w", stdout);*/
    /*vector<ll> fac(200005 ,0);
    fac[0] = 1ll;
    fac[1] =1ll;
    for(ll i=2;i<200005;i++)
    {
        fac[i] = ((fac[i-1]%NUM)*(i%NUM))%NUM;
    }*/
    //cout<<fac[199999]<<endl;
    int t ;
    cin>>t;
    while(t--)
    {
        solve();
    }
    return 0;
    
    
    //return 0;
}

