        //v[0] = y-x;
        ll num =1;
        ll ans = 0;
        for(ll i=0;i<10;i++)
        {
            ll diff;
            ll l = x/num;
            ll r = y/num;
            v[i] = r - l;
            ans +=v[i];
            num = num*10;
        }
        cout<<ans<<endl;
        
    }
    return 0;
}
