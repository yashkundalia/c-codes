#include <iostream>
#include <map>
#include<unordered_map>
#include <iterator>
#include <vector>
#include <string>
#include <algorithm>
#include<utility>
#include<climits>
#include<stack>


using namespace std;




int main()
{
    int n,t;
    cin>> n >> t;
    if(n==1 and t==10)
    {
        cout<< -1 << endl;
        return 0;
    }
    vector<char> v(n,'0');
    string temp = to_string(t%10);
    if(t!=10) v[0] = temp[0];
    else v[0] = '1';
    string ans ="";
    for(int i=0;i<n;i++)
    {
        ans += v[i];
    }
    cout<<ans<<endl;
    return 0;
}

