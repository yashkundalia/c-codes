#define ull unsigned long long
#define ll long long
//#define NUM 1000000001
//ull mod  = 998244353;
using namespace std;

int prime(ll num)
{
    if(num <=1) return 0;
    for(ll i=2;i*i<=num;i++)
    {
        if(num % i == 0 ) return 0;
    }
    return 1;
}
int main()
{
    int t = 1;
    //cin >> t;
    while (t--) 
    {
        ll n,m,k;
        cin>>n>>m>>k;
        vector<vector<ll> > v(n + 1,vector<ll> (m + 1,0));
        for(ll i=1;i<=n;i++)
        {
            for(ll j=1;j<=m;j++)
            {
                ll temp;
                cin>>temp;
                v[i][j] = temp;
            }
        }
        vector<ll> pos(k,0);
        for(ll i=0;i<k;i++)
        {
            ll temp;
            cin>>temp;
            pos[i] = temp;
        }
        vector<ll> ans;
        for(ll i=0;i<k;i++)
        {
            ll row = 1;
            ll col = pos[i];
            while(row<=n)
            {
                if(v[row][col] == 1)
                {
                    v[row][col] = 2;
                    col = col + 1;
                }
                else if(v[row][col] == 2)
                {
                    if(row == n)
                    {
                        cout<<col<<" ";
                        break;
                    }
                    row++;

                }
                else if(v[row][col] == 3)
                {
                    v[row][col] = 2;
                    col--;
                }
            }
            //ans.push_back(col);
            /*if(v[row][col] == 1)
            {
                v[row][col] = 2;
                cout<<col + 1<<" ";
            }
            else if(v[row][col] == 3)
            {
                v[row][col] = 2;
                cout<<col - 1<<" ";
            }
            else
            {
                cout<<col<<" ";
            }
*/

        }
        cout<<endl;
    }
    
    
    return 0;
}
