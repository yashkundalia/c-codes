vector<int> duplicates(int arr[], int n) {
        // code here
        vector<int> ans;
        sort(arr,arr+n);
        int num =0;
        for(int i=0;i<n-1;i++)
        {
            
            if(arr[i+1]==arr[i] and num==0)
            {
                ans.push_back(arr[i]);
                num=1;
            }
            else if(arr[i+1]!=arr[i]) num =0;
        }
        if (ans.size() ==0)
        {
            ans.push_back(-1);
        }
        return ans;
    }
