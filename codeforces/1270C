#define ull unsigned long long
#define ll long long
#define NUM 1000000007
//ll mod  = 1000000007;
using namespace std;



ll binpow(ll x, ll y)
{
    ll res = 1ll; // Initialize result
  
    while (y > 0ll) {
        // If y is odd, multiply x with result
        if (y & 1ll)
            res = ((res%NUM) * (x%NUM))%NUM;
  
        // y must be even now
        y = y >> 1; // y = y/2
        x = ((x%NUM) * (x%NUM))%NUM; // Change x to x^2
    }
    return res%NUM;
}

bool sortbysec(pair<ll,ll> &a , pair<ll,ll> &b)
{
    if(a.first == b.first)
    {
        return (a.second <= b.second);
    }
    return (a.first < b.first) ;
}
 
//vector<bool> ch(1025,false);
//vector<int> num(1025,0);
bool check(int k,vector<int> v,vector<bool> ch)
{
    int n = v.size();
    for(int i=0;i<n;i++)
    {
        if(!ch[v[i]^k]) return false;
    }
    return true;
}

void solve()
{
    ll n;
    cin>>n;
    vector<ll> v;
    ll sum =0;
    ll x = 0;
    for(ll i=0;i<n;i++)
    {
        ll temp;
        cin>>temp;
        v.push_back(temp);
        sum+=temp;
        x = x^temp;
    }
    cout<<2<<endl;
    cout<<sum+x <<" "<<x<<endl;
    return;

}




int main()
{
    
    int t ;
    cin>>t;
    while(t--)
    {
        solve();
    }
    return 0;
    
}

