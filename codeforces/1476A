#include <iostream>
#include <map>
#include<unordered_map>
#include <iterator>
#include <vector>
#include <string>
#include <algorithm>
#include<utility>
#include<climits>
#include<stack>

#define ull unsigned long long
#define ll long long

using namespace std;




int main()
{
    ull t;
    cin>>t;
    while(t--)
    {
        ull n,k;
        cin>>n>>k;
        if(n==k)
        {
            cout<<1<<endl;
            continue;
        }
        if(n > k)
        {
            if(n%k == 0) cout<<1<<endl;
            else cout<<2<<endl;
            continue;
        }
        if(n<k)
        {
            //cout<<k/n + k%n<<endl;
            //continue;
            if(k%n == 0) cout<<k/n<<endl;
            else cout<<k/n + 1<<endl;
        }
    }
    return 0;
}

