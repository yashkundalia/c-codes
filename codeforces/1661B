#include <iostream>
#include <map>
#include<unordered_map>
#include <iterator>
#include <vector>
#include <string>
#include <algorithm>
#include<utility>
#include<climits>
#include<stack>

#define ull unsigned long long
#define ll long long
//#define NUM 1000000001
//ull mod  = 998244353;
using namespace std;


void solve()
{
    ll n;
    cin>>n;
    vector<ll> v ;
    for(ll i=0;i<n;i++)
    {
        ll temp;
        cin>>temp;
        v.push_back(temp);
    }
    vector<ll> ans;
    for(ll i=0;i<n;i++)
    {
        ll num = v[i];
        //ll d = 32768/2;
        ll a = 15;
        for(ll i = 0;i<=15;i++)
        {
            ll mult = 1;
            for(ll j = 0;j<=15;j++)
            {
                
                if(((num + i)*mult)%32768 == 0)
                {
                    a = min(a , i+j);
                }
                mult = 2*mult;
            }
        }
        ans.push_back(a);
    }
    for(auto x : ans)
    {
        cout<<x<<" ";
    }
    cout<<endl;
    return;
    

}
int main()
{
    int t =1;
    //cin>>t;
    while(t--)
    {
        solve();
    }
    return 0;
}
