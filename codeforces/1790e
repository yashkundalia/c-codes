#include<cmath>
#include<numeric>


#define ull unsigned long long
#define ll long long
#define NUM 1000000007
//ull mod  = 998244353;
using namespace std;


bool sortbysec(pair<ll,ll> &a , pair<ll,ll> &b)
{
    if(a.first == b.first)
    {
        return (a.second <= b.second);
    }
    return (a.first < b.first) ;
}

void solve()
{
    ll x;
    cin>>x;
    ll a =x;
    ll b =0;
    for(ll i =32;i>=0;i--)
    {
        ll num = x&(1ll<<i);
        //cout<<num<<"num"<<endl;
        if(num > 0 ) continue;
        num = (2ll<<i) ;
        //cout<<num<<"num"<<endl;
        if(2*x - a -b >= num)
        {
            //cout<<i<<"i"<<endl;
            //cout<<2*x - a-b<<"diff"<<endl;
            a += 1<<i ;
            b += 1<<i ;
            //cout<<a<<" "<<b<<endl;
        }
        if(a+b == 2*x and a^b == x)
        {
            cout<<a<<" "<<b<<endl;
            return;
        }
    }
    cout<<-1<<endl;
    return ;


}



int main()
{
    
    int t ;
    cin>>t;
    //t=1;
    while(t--)
    {
        solve();
    }
    return 0;
}

